{
    "name": "snscripts/timezones",
    "description": "Timezone helpers to convert to and from UTC with Laravel Blade directives",
    "keywords": ["Timezone", "UTC", "Conversion", "Helpers", "Laravel", "DateTime"],
    "homepage": "https://github.com/mikebarlow/Timezones",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Barlow",
            "email": "mike@mikebarlow.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.5",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Snscripts\\Timezones\\": "src"
        },
        "classmap": [
            "src/Facade/Timezones.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Snscripts\\Timezones\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Snscripts\\Timezones\\TimezonesServiceProvider"
            ]
        }
    }
}